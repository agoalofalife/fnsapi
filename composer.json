{
    "name": "agoalofalife/fnsapi",
    "description": "Interaction with API FNS",
    "keywords": [
        "fns",
        "soap"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "agoalofalife",
            "email": "agoalofalife@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "psr/simple-cache": "^1.0",
        "ext-json": "*",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Fns\\" : "src/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure --no-coverage"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "mockery/mockery": "^1.3@dev",
        "fzaninotto/faker": "^1.9@dev"
    }
}
